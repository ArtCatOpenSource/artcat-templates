<div id="FullColumn">
  {% for image_set in image_sets %}
    <div class="thumb-float">
      <div class="thumb-img">
        <a href="{{ image_set.page_url }}"><img style="max-width: 140px" src="{{ image_set.main_thumb_url }}" alt="{{image_set.name | escape}}" /></a>
      </div>
      <p class="thumb-caption">{{image_set.name | escape}}</p>
    </div>
  {% endfor %}
</div>
